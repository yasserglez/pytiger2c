let
	type intArray = array of int
	var a : intArray := intArray[1] of 0
	var b : intArray := intArray[2] of 2
	type intMat = array of intArray
	var c := intMat[20] of intArray[20] of -1
in
	a:= b;
	b[1]:= c[1][2];
	for j:= 1 to 19 do
	(
		for k:= 1 to 19 do
			printi(c[j][k]);
		print("\n")
	)
end